#include <cstdio>
#include <cstring>
using namespace std;
main(){
       int a,b=0,c,d=0,k=0,j;
       char u[10];
       scanf("%s",&u);
       a=strlen(u);
       j=strlen(u);
       while(b<=j/2){
            if(u[b]==u[a-1]){
                d++;}
            if(d==((j/2)+1)){
                k++;
                b=j;}
            a=a-1;
            b++;}
       if(k==1){
            printf("The word you entered is palindrom");}
       if(k!=1){
            printf("The word you entered is not palindrom");}
       for(;;);
}
